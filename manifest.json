{
  "name": "Rhenium Chrome Extension Checker",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Chrome Extension Checker",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs",
    "*://fdsa.jp/*",
    "https://chrome.google.com/webstore/detail/*"
  ],
  "page_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "now scan"
  }
}
